FoamFile
{
    version         1.4;
    format          ascii;

    root            "/export/lime/data-04/sws02hs/OpenFOAM/sws02hs-1.4.1/run/shallowWater";
    case            "bucky4_30";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
projection "X9c/9c";
projection2D XY;

FieldsToPlot
1
(
    [Tdiff filledContours -0.095 0.095 0.01 "$GMTU/colours/red_white_blue.cpt"]
);

// options
overlay false;
lastPlot false;
//region "0/1e4/0/1e4";
region "0/10e3/0/9.99e3";
annotate "minMax 2000 500";
boundaryMarks "a2e3/a2e3";
separateLegend true;
plotBoundaryDots false;

